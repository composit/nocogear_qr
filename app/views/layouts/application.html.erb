<!DOCTYPE html>
<html>
<head>
  <title>SinglePageQr</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if notice %>
    <div id='notice'><%= notice %></div>
  <% end %>
  <% if alert %>
    <div id='alert'><%= alert %></div>
  <% end %>

<%= yield %>

</body>
</html>
