<h1>Listing entries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @entries.each do |entry| %>
      <tr>
        <td><%= entry.name %></td>
        <td><%= entry.email %></td>
        <td><%= entry.phone %></td>
        <td><%= entry.description %></td>
        <td><%= link_to 'Show', [:admin, entry] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
